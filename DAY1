#4.https://practice.geeksforgeeks.org/problems/swap-the-numbers/1/?track=cip-java-intro-var-operator&batchId=197
import java.util.*;
import java.io.*;

class GFG
{
    public static void main(String args[])
    {
        Scanner sc= new Scanner(System.in);
        int t = sc.nextInt();
        while(t-->0){
            int a = sc.nextInt();
            int b = sc.nextInt();
            Solution oj = new Solution();
            oj.utility(a,b);
        }
    }
}
// } Driver Code Ends


//User function Template for Java


class Solution
{
    public void utility(int a, int b){
        
        int temp=0;
        
        temp= a;
         a = b;
         b = temp;
        
        System.out.println(a+" "+b);
    }
}

#5.https://practice.geeksforgeeks.org/problems/sum-of-n-number/1/?track=cip-java-intro-var-operator&batchId=197
int nSum(int n){
    int ans = 0;
    
    for(int i=1;i<=n;i++)
    {
      ans= ans+i;
    }
      

    //Write your code here to calculate and return sum of n number.
    return ans;
    
}

