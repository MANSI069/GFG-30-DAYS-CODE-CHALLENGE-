		int t = sc.nextInt();
		
		while(t-- > 0){
		    System.out.println(sc.nextInt());
		    System.out.println(sc.nextFloat());
		    System.out.println(sc.nextLong());
		    System.out.println(sc.nextByte());
		    sc.nextLine();
		    System.out.println(sc.nextLine());
		    
		  
#2.https://practice.geeksforgeeks.org/problems/data-types-java/1/?track=cip-java-intro-var-operator&batchId=197

class Geeks{
    
    // Function to do operations with different data types
    static void dataTypes(int a, float b, double c, long l, byte d){
        
        double p = c/b ;//c/b
        double q = b/a;//b/a
        double r = c/a;//c/a
        double m = r+l;//r+l
        int s = a/d ;//a/d
        
        
        
        
        //Printing all the results 
        System.out.println(p + " " + q + " " + r + " " + m + " " + s);
        
    }
}

